{
  "name": "tgs-cli",
  "description": "TagSet CLI",
  "version": "0.1.0",
  "author": "AlexanderLapygin @AlexanderLapygin",
  "bin": {
    "tgs": "./bin/run"
  },
  "bugs": "https://github.com/MindSection/tgs-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.16",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/MindSection/tgs-cli/blob/master/README.md",
  "keywords": [
    "cli",
    "MindSection",
    "node",
    "tag",
    "tagset",
    "tgs"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tgs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MindSection/tgs-cli.git"
  },
  "scripts": {
    "postpack": "rimraf tgs-cli.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
